{
  "name": "ng2-renuo-upload",
  "version": "0.0.1",
  "description": "Component for the Renuo Upload",
  "keywords": [
    "angular2",
    "webpack",
    "typescript"
  ],
  "author": "Renuo <info@renuo.ch>",
  "homepage": "https://github.com/renuo/vh-webav-frontend",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && webpack --progress --profile --bail",
    "ci": "npm run test",
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "docker-dev": "webpack-dev-server --inline --port 3000 --host 0.0.0.0 --progress --profile --bail",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts",
    "lint": "tslint -c tslint.json \"src/**/*.ts\" --fix",
    "postinstall": "npm run webdriver-update",
    "pree2e": "npm run webdriver-update -- --standalone",
    "server": "webpack-dashboard -- webpack-dev-server --inline --port 3000",
    "server-without-dashboard": "webpack-dev-server --inline --port 3000",
    "server:prod": "http-server -p 3000 dist --cors",
    "start": "npm run server",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "watch": "webpack --watch --progress --profile",
    "webdriver-start": "webdriver-manager start",
    "webdriver-update": "webdriver-manager update"
  },
