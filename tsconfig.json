{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noEmitHelpers": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictNullChecks": false,
    "skipLibCheck": false,
    "types": [
      "core-js",
      "jasmine",
      "lodash",
      "selenium-webdriver",
      "node"
    ]
  },
  "compileOnSave": false,
  "buildOnSave": false
}
